<div class="title p-3 d-flex justify-content-center">
    <i class="bi bi-lightbulb-fill cursor-pointer text-muted me-2"></i>
    <strong>Examples
    </strong>
</div>
<div class="d-flex flex-wrap justify-content-between">
    <% @examples.each do |example| %>
        <div class="col-12 col-md-4 d-flex flex-column mb-3">
            <div class="theme-bg-secondary rounded px-3 py-2 mx-2 cursor-pointer border-0">
            <%= example %>
            </div>
        </div>
    <% end %>
</div>
